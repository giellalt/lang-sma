<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE errors>

<errors>

  <defaults>
    <default type="Grammar error">
      <ids>
        <re v="real-.*"/>
        <e id="unreal-girjji"/>
      </ids>
      <header>
        <title xml:lang="en">Real-word error</title>
        <title xml:lang="sma">[SMA]Čállinmeattáhus</title>
      </header>
      <body>
        <description xml:lang="en">The word "$1" exists in the dictionary, but means something different in this context.</description>
        <description xml:lang="sma">[SMA]"$1" maŋis galggašii leat blah</description>
      </body>
    </default>

    <default type="Grammar error">
      <ids>
        <e id="default"/>
      </ids>
      <header>
        <title xml:lang="en">Grammar error</title>
        <title xml:lang="sma">[SMA]Čállinmeattáhus</title>
      </header>
      <body>
        <description xml:lang="en">The word "$1" exists in the dictionary, but means something different in this context.</description>
        <description xml:lang="sma">[SMA]"$1" maŋis galggašii leat blah</description>
      </body>
    </default>
  </defaults>

  <error id="real-override">
    <header>
      <title xml:lang="en">cake instead of cheese</title>
      <title xml:lang="sma">[SMA]gáhkku iige vuostá</title>
    </header>
    <body>
      <description xml:lang="en">this should be "cake" instead of "cheese"</description>
      <description xml:lang="sma">[SMA]galggašii leat "gáhkku" iige "vuostá"</description>
    </body>
  </error>

  <error id="msyn-valency-ill">
    <header>
      <title xml:lang="en">illative instead of accusative</title>
      <title xml:lang="sma">[SMA]illatiiva iige akkusatiiva</title>
      <references> <ref n=""/></references>
    </header>
    <body>
      <description xml:lang="en">illative instead of accusative</description>
      <description xml:lang="sma">[SMA]illatiiva iige akkusatiiva</description>
    <examples>
      <ex xml:lang="sma">[SMA]Hui ollu liikon {bohccobierggu} ja {goikebierggu}.</ex>
    </examples>
    </body>
  </error>


  <error id="msyn-valency-acc">
    <header>
      <title xml:lang="en">accusative instead of nominative</title>
      <title xml:lang="sma">[SMA]akkusatiiva iige nominatiiva</title>
      <references> <ref n=""/></references>
    </header>
    <body>
      <description xml:lang="en">accusative instead of nominative</description>
      <description xml:lang="sma">[SMA]illatiiva iige nominatiiva</description>
    <examples>
      <ex xml:lang="sma">[SMA]Mun čálán {reive}.</ex>
    </examples>
    </body>
  </error>

  <error id="msyn-valency-loc-com">
    <header>
      <title xml:lang="en">msyn thing</title>
      <title xml:lang="sma">[SMA]msyn thingy</title>
      <references><ref n=""/></references>
    </header>
    <body>
      <description xml:lang="en">wrong word</description>
      <description xml:lang="sma">[SMA]boasttut sátni</description>
    <examples>
      <ex xml:lang="sma">[SMA]Ovdalgihtiigeassima sierra máksingoartta ferte álohii atnit go máksá boastagiro dahje báŋku {badjel}.</ex>
    </examples>
    </body>
  </error>

  <error id="lex-bokte-not-badjel">
    <header>
      <title xml:lang="en">"bokte" not "badjel"</title>
      <title xml:lang="sma">[SMA]"bokte" iige "badjel"</title>
      <references><ref n=""/></references>
    </header>
    <body>
      <description xml:lang="en">wrong word</description>
      <description xml:lang="sma">[SMA]boasttut sátni</description>
    <examples>
      <ex xml:lang="sma">[SMA]Ovdalgihtiigeassima sierra máksingoartta ferte álohii atnit go máksá boastagiro dahje báŋku {badjel}.</ex>
    </examples>
    </body>
  </error>

  <error id="syn-pred-not-attr-msg">
    <header>
      <title xml:lang="sma">[SMA]«$1» ja «$2» leaba blah</title>
    </header>
    <body>
      <description xml:lang="sma">[SMA]Predicative, iige attributive – «$1» ja «$2» leaba blah</description>
    </body>
  </error>

  <error id="msyn-valency-go-inf">
    <header>
      <title xml:lang="en">Try a subclause with <em>go</em> instead of illative</title>
      <title xml:lang="sma">[SMA]Iskka<em>go</em>-oalgecealkaga iige infinitiiva</title>
      <references> <ref n=""/></references>
    </header>
    <body>
      <description xml:lang="en"><em>go</em> instead of illative</description>
      <description xml:lang="sma">[SMA]<em>go</em> iige infinitiiva</description>
    <examples>
      <ex xml:lang="sma">[SMA]Soai leaigga sihke dolkan ja suhttan gullat olbmuid šláddariid sudno birra, ja bovdiiga.</ex>
    </examples>
    </body>
  </error>

  <error id="punct-aistton">
    <header>
      <title xml:lang="en">try other quotation marks</title>
      <title xml:lang="sma">[SMA]iskkas eará aisttonmearkkat</title>
      <references><ref n=""/></references>
    </header>
    <body>
      <description xml:lang="en">try other quotation marks</description>
      <description xml:lang="sma">[SMA]iskkas eará aisttonmearkkat</description>
    <examples>
      <ex xml:lang="sma">[SMA]Doarjja addo vuosttažettiin {«} Sámi dutkama prográmmii {»} ( 2001–05 ).</ex> 
    </examples>
    </body>
  </error>

  <error id="typo">
    <header>
      <title xml:lang="en">"$1" seems misspelled</title>
      <title xml:lang="sma">[SMA]"$1" lea čállinmeattáhus</title>
    </header>
    <body>
      <description xml:lang="en">The word "$1" seems to be misspelled.</description>
      <description xml:lang="sma">[SMA]Sáni "$1" dáidá leat čállinmeattáhus.</description>
    </body>
  </error>

  <error id="double-space-before">
    <header>
      <title xml:lang="en">Two spaces between "$2" and "$1"</title>
      <title xml:lang="sma">[SMA] Two spaces between "$2" and "$1"</title>
    </header>
    <body>
      <description xml:lang="en">There are two spaces in front of "$1"</description>
      <description xml:lang="sma">[SMA] There are two spaces in front of "$1"</description>
    </body>
  </error>

  <error id="space-after-paren-beg">
    <header>
      <title xml:lang="en">Space after opening parenthesis</title>
      <title xml:lang="sma">[SMA] Space after opening parenthesis</title>
    </header>
    <body>
      <description xml:lang="en">There is a space after the opening parenthesis "$1"</description>
      <description xml:lang="sma">[SMA] There is a space after the opening parenthesis "$1"</description>
    </body>
  </error>

  <error id="space-before-paren-end">
    <header>
      <title xml:lang="en">Space before closing parenthesis</title>
      <title xml:lang="sma">[SMA] Space before closing parenthesis</title>
    </header>
    <body>
      <description xml:lang="en">There is a space before the closing parenthesis "$1"</description>
      <description xml:lang="sma">[SMA] There is a space before the closing parenthesis "$1"</description>
    </body>
  </error>

  <error id="space-before-punct-mark">
    <header>
      <title xml:lang="en">Space before punctuation</title>
      <title xml:lang="sma">[SMA] Space before punctuation</title>
    </header>
    <body>
      <description xml:lang="en">There is a space in front of the punctuation mark "$1"</description>
      <description xml:lang="sma">[SMA] There is a space in front of the punctuation mark "$1"</description>
    </body>
  </error>

  <error id="no-space-after-punct-mark">
    <header>
      <title xml:lang="en">Missing space</title>
      <title xml:lang="sma">[SMA] Missing space</title>
    </header>
    <body>
      <description xml:lang="en">There is no space after the punctuation mark "$1"</description>
      <description xml:lang="sma">[SMA] There is no space after the punctuation mark "$1"</description>
    </body>
  </error>

</errors>
