<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE errors>

<errors>

  <defaults>
    <default type="Grammar error">
      <ids>
        <re v="real-.*"/>
      </ids>
      <header>
        <title xml:lang="en">Spelling error?</title>
        <title xml:lang="sma">Båajhtode tjaalasovveme?</title>
      </header>
      <body>
        <description xml:lang="en">The word "$1" seems to be misspelled.</description>
        <description xml:lang="sma">"$1"-baakoe vååjnoe båajhtode.</description>
      </body>
    </default>

    <default type="Grammar error">
      <ids>
        <e id="default"/>
      </ids>
      <header>
        <title xml:lang="en">Spelling error?</title>
        <title xml:lang="sma">Båajhtode tjaalasovveme?</title>
      </header>
      <body>
        <description xml:lang="en">The word "$1" seems to be misspelled.</description>
        <description xml:lang="sma">"$1"-baakoe vååjnoe båajhtode.</description>
      </body>
    </default>
  </defaults>

  <error id="punct-aistton">
    <header>
      <title xml:lang="en">Wrong quotation mark?</title>
      <title xml:lang="sma">Båajhtoeh jiehtege-væhta?</title>
      <references><ref n=""/></references>
    </header>
    <body>
      <description xml:lang="en">The quotation mark seems not to follow the Sámi standard</description>
      <description xml:lang="sma">Jiehtege-væhta ij leah saemien maallen mietie</description>
    </body>
  </error>
  
  <error id="guillemets">
    <header>
      <title xml:lang="en">Wrong quotation mark?</title>
      <title xml:lang="sma">Båajhtoeh jiehtege-væhta?</title>
      <references><ref n=""/></references>
    </header>
    <body>
      <description xml:lang="en"></description>
      <description xml:lang="sma"></description>
    </body>
  </error>

  <error id="typo">
    <header>
      <title xml:lang="en">"$1" seems misspelled</title>
      <title xml:lang="sma">"$1"-baakoe båajhtode tjaalasovveme</title>
    </header>
    <body>
      <description xml:lang="en">The word "$1" seems to be misspelled.</description>
      <description xml:lang="sma">"$1"-baakoe vååjnoe båajhtode.</description>
    </body>
  </error>

  <error id="double-space-before">
    <header>
      <title xml:lang="en">Two spaces in front of "$1"</title>
      <title xml:lang="sma">Göökte væhtagaskeh "$1"-baakoen uvte</title>
    </header>
    <body>
      <description xml:lang="en">There are two spaces between "$2" and "$1"</description>
      <description xml:lang="sma">Göökte væhtagaskeh "$2" jïh "$1"-baakoej gaskem</description>
    </body>
  </error>

  <error id="space-after-paren-beg">
    <header>
      <title xml:lang="en">Space after opening parenthesis</title>
      <title xml:lang="sma">Væhtagaske gårroehparanteesen minngesne</title>
    </header>
    <body>
      <description xml:lang="en">There is a space after the opening parenthesis</description>
      <description xml:lang="sma">Væhtagaske gårroehparanteesen minngesne</description>
    </body>
  </error>

  <error id="space-before-paren-end">
    <header>
      <title xml:lang="en">Space before closing parenthesis</title>
      <title xml:lang="sma">Væhtagaske åelkiesparanteesen minngesne</title>
    </header>
    <body>
      <description xml:lang="en">There is a space before the closing parenthesis</description>
      <description xml:lang="sma">Væhtagaske åelkiesparanteesen minngesne</description>
    </body>
  </error>

  <error id="space-before-punct-mark">
    <header>
      <title xml:lang="en">Space before punctuation</title>
      <title xml:lang="sma">Væhtagaske tjaelemevæhtan uvte</title>
    </header>
    <body>
      <description xml:lang="en">There is a space in front of the punctuation mark "$1"</description>
      <description xml:lang="sma">Væhtagaske tjaelemevæhtan uvte</description>
    </body>
  </error>

  <error id="no-space-after-punct-mark">
    <header>
      <title xml:lang="en">Missing space</title>
      <title xml:lang="sma">Væhtagaske fååtese</title>
    </header>
    <body>
      <description xml:lang="en">There is no space after the punctuation mark "$1"</description>
      <description xml:lang="sma">Ij naan væhtagaske "$1"-væhtan minngesne</description>
    </body>
  </error>

  <error id="msyn-agr-other-sg1">
    <header>
      <title xml:lang="en">First person singular after 'mun'.</title>
      <title xml:lang="sma">Verbe vuossâmuu persovnist.</title>
      <title xml:lang="nb">Verb i første person.</title>
    </header>
    <body>
      <description xml:lang="en">The verb-form "$1" follows 'manne', and should probably be first person singular.</description>
      <description xml:lang="sma">Verbehammoe "$1" åtna 'manne'-baakoen mænngan. Mejtie voestes persovne buerebe?</description>
      <description xml:lang="nb">Verbforma "$1" blir brukt etter ordet 'mun' . Hadde det vært bedre å bruke 1. person?</description>
    </body>
  </error>

  <error id="msyn-agr-other-sg3">
    <header>
      <title xml:lang="en">Third person singular after singular subject.</title>
      <title xml:lang="sma">Verbâ 3. persovnist.</title>
      <title xml:lang="nb">Verb i tredje person.</title>
    </header>
    <body>
      <description xml:lang="en">The verb-form "$1" follows a third person subject, and should probably be third person singular.</description>
      <description xml:lang="sma">Verbehammoe "$1" åtna substantijven mænngan. Mejtie buerebe gåalmadinie persovnine?</description>
      <description xml:lang="nb">Verbforma "$1" blir brukt etter et ord i entall . Hadde det vært bedre å bruke 3. person?</description>
    </body>
  </error>

  <error id="msyn-adj-pred-attr">
    <header>
      <title xml:lang="en">Adjective in attributive form.</title>
      <title xml:lang="sma">Adjektiive attribuvtehammoesne.</title>
      <title xml:lang="nb">Adjektiv i attributtform.</title>
    </header>
    <body>
      <description xml:lang="en">The adjective "$1" preceeds a noun, and should probably be attributive.</description>
      <description xml:lang="sma">Adjektijvehammoe "$1" substantijven uvte tjåådtje, guktie tjoevere atribuhtehammosne tjåadtjodh.</description>
      <description xml:lang="nb">Adjektivforma "$1" står foran et substantiv, og skulle sannsynligvis ha hatt ei attributtform.</description>
    </body>
  </error>

  <error id="msyn-adj-attr-pred">
    <header>
      <title xml:lang="en">Adjective in predicative form.</title>
      <title xml:lang="sma">Adjektiive predikatiiveehammoesne.</title>
      <title xml:lang="nb">Adjektiv i predikativ form.</title>
    </header>
    <body>
      <description xml:lang="en">The adjective "$1" probably stands in predicative position, and should thus be in the predicative form.</description> 
      <description xml:lang="sma">Adjektivforma "$1" byöroe predikatijvine årrodh.</description>
      <description xml:lang="nb">Adjektivforma "$1" er sannsynligvis predikativ, og bør i tilfelle stå i predikativform.</description>
    </body>
  </error>


  <error id="msyn-adj-attr-adv">
    <header>
      <title xml:lang="en">Adjective should be adverb.</title>
      <title xml:lang="sma">Adjektiive byöroe adverbine årrodh.</title>
      <title xml:lang="nb">Adjektiv skal være adverb.</title>
    </header>
    <body>
      <description xml:lang="en">The adjective "$1" should probably be an adverb.</description> 
      <description xml:lang="sma">Adjektijvehammoe "$1" byöroe adverbine årrodh.</description>
      <description xml:lang="nb">Adjektivforma "$1" burde sannsynligvis være et adverb.</description>
    </body>
  </error>
  

  <error id="msyn-Neg-ConNeg-Congr-DuPl">
