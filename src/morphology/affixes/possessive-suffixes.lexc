        +Ela+PxSg1:%>estanne FINAL1 ;
 +Ela+PxSg1+Use/NG:%>estadth FINAL1 ;
        +Com+PxSg2:%>enadth  FINAL1 ;
        +Com+PxSg3:%>ebinie  FINAL1 ; ! gaameginie
        +Ess+PxSg3:%>ebinie  FINAL1 ; ! gaameginie


