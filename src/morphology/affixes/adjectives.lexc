! Attribute lexica
! ----------------

LEXICON ATTR_0_PRED_0
   ATTR_0 ;
   PRED_0 ;

LEXICON ATTR_0
      +A: ATTRCONT ;


LEXICON ATTR_0_ODD
      +A:e ATTRCONT ;
