!          ODDCASEOBL ;  - predikativ: 'nahkeren' 'geeruvisnie' - i adv.og noun-fila

LEXICON EENSI
      +A:i  ATTRCONT ;
        :ig ODDCOMP  ;

LEXICON YÖLELH
      +A:h ATTRCONT ;
           ODDCOMP  ;

LEXICON ATTR_S
      +A:%>s ATTRCONT ;

LEXICON ATTR_H
     +A:%>h ATTRCONT ;

LEXICON ATTR_N
      +A:%>n ATTRCONT ;


LEXICON ATTRCONT
 +Attr:                                 FINAL1         ;
 +Cmp/Attr:                                 R              ;
 +Attr+Der2+Der/vuota+N+Err/Orth:%»voet GIERIEHTSADTJE ;
 +Attr+Der2+Der/vuota+N+Use/MT:%»voet GIERIEHTSADTJE ;
