!***** Adjektiver som skiller seg ut fra systemet****!

LEXICON REAKTOE
 :reakt   oeCASE ;
 :rïekt   es_ES_EVENNOCOMP1 ;
 

LEXICON JAEDTUVES
            :uves           ATTR_0     ;
       +A+Sg:               ÅERUVE_SG  ;
       +A+Pl:               ÅERUVE_PL  ;
          +A:               ÅERUVE_ESS ;
 +A+Cmp/SgNom:uve%^DISIMP    R          ;
 +A+Cmp/SgGen:uve%>%^DISIMPn R          ;
 +A+Cmp/PlGen:uv%>%^DISIMPi  R          ;
            :iev            ODDCOMP    ;

LEXICON VIELIEH
 :lieh ATTR_0 ;
 :lieh PRED_0 ;

