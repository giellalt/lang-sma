!! !!!The auxiliaries
! ===============

!! First we just list the auxiliaries and their inflection.

LEXICON LEA  !!= * __@CODE@__ the copula

!+Inf:lea   FINAL1 ; ! Uses årrodh
        +Ind+Prs:l       LEA-PRES ;
        +Ind+Prt:l       LEA-PRET ;
 +Ind+ConNeg+Prt:l       LEA-PRET ;
          +Imprt:l       LEA-IMP  ;
         +ConNeg:leah    FINAL1   ;
   +Cond+Prs+Sg3:lidtjie FINAL1   ; ! Magkeres lidtjie deere orreme?

LEXICON LEA-PRES  !!= ** __@CODE@__
 +Sg1:eam   FINAL1 ;
 +Sg2:eah   FINAL1 ;
 +Sg3:ea    FINAL1 ;
 +Du1:ean   FINAL1 ;
 +Du2:idien FINAL1 ;
 +Du3:ægan  FINAL1 ;
!+Du3:æban   FINAL1  ; !SUB
 +Pl1:ibie  FINAL1 ;
 +Pl2:idie  FINAL1 ;
 +Pl3:eah   FINAL1 ;

LEXICON LEA-PRET !!= ** __@CODE@__
 +Sg1:im   FINAL1 ;
 +Sg2:ih   FINAL1 ;
 +Sg3:ij   FINAL1 ;
 +Du1:imen FINAL1 ;
 +Du2:iden FINAL1 ;
 +Du3:igan FINAL1 ;
 +Pl1:imh  FINAL1 ;
 +Pl2:idh  FINAL1 ;
 +Pl3:in   FINAL1 ;

LEXICON LEA-IMP !!= ** __@CODE@__
 +Sg2:eah   FINAL1 ;
 +Du2:idien FINAL1 ;
 +Pl2:idie  FINAL1 ;


!! !!The negative verb
! =================

LEXICON NEG  !!= * __@CODE@__
      +Ind+Prs:     IJ-PRES   ;
      +Ind+Prt:idtj VSUF-PRT  ;
        +Imprt:aell NEGIMP    ;
 +Imprt+Use/NG:oll  OLLE      ;
      +Ind+Prt:     IJ-COPULA ;



!Negation "var ikke" -  is not in, but we have here three kind of negationsystems
!AJ= Anna Jacobsen
!KB= Knut Bergsland


!SG
!         -AJ-      KB         -Røros
!1.sg: im lih  - im lim      - eedtjem lih (merknader s.45 i Bergsland)
!2.sg: ih lih - ih lih       - eedtjh lih (merknader s.45 i Bergsland)
!3.sg: ij lij - ij lih       - eedtji lih (merknader s.45 i Bergsland)

!Dual
!          - AJ         - KB-
!1.dual: ij limen - ean limen
!2.dual: ij liden - idien liden
!3.dual: ij ligan - eakan ligan

! Plural
!        -AJ     -  KB -
!1.pl:  ij limh  - ibie limh      (merknader s.45 på også ij limh i Bergsland)
!2.pl:  ij lidh  - idie lidh
!3.pl:  ij lin   - eah lin

! And her are some eks.
!ij lim(b) manne gåetesne dellie - this one is like KB's 1.p.sg.
!datne ih lih gåetesne gænnah
!Piere ij lih gåetesne
!
!ij limen månnoeh gåetesne gænnah
!ij liden dåtnoeh gåetesne
!Dah guaktah ij lin/ligan gåetesne gænnah
!
!Ij limh mijjieh gåetesne gænnah
!Dijjieh ij lidh gåetesne gænnah
!Dah ij lin gåetesne gænnah


LEXICON OLLE  !!= ** __@CODE@__
 +Sg1:em   FINAL1 ;
 +Sg2:h    FINAL1 ;
 +Sg3:es   FINAL1 ;
 +Du1:en   FINAL1 ;
!+Du2:ijidien    FINAL1 ;
 +Du2:eden FINAL1 ;
 +Du3:es   FINAL1 ;
 +Pl1:ebe  FINAL1 ;
!+Pl1:ijibie    FINAL1 ;
!+Pl2:ijidie    FINAL1 ;
 +Pl2:ede  FINAL1 ;
 +Pl3:es   FINAL1 ;


LEXICON NEGIMP  !!= ** __@CODE@__
 +Sg1:iem  FINAL1 ;
 +Sg2:ieh  FINAL1 ;
 +Sg3:ies  FINAL1 ;
 +Du1:ien  FINAL1 ;
 +Du2:eden FINAL1 ;
!+Du2:iejidien   FINAL1 ;
 +Du3:ies  FINAL1 ;
 +Pl1:ebe  FINAL1 ;
 +Pl2:ede  FINAL1 ;
!+Pl2:iejidie    FINAL1 ;
 +Pl3:ies  FINAL1 ;

LEXICON IJ-PRES  !!= ** __@CODE@__
         +Sg1:im    FINAL1 ;
         +Sg2:ih    FINAL1 ;
         +Sg3:ij    FINAL1 ;
 +Sg3+Use/Sub:ii    FINAL1 ;

         +Du1:ean   FINAL1 ;
         +Du2:idien FINAL1 ;
         +Du3:eakan FINAL1 ;
         +Pl1:ibie  FINAL1 ;
         +Pl2:idie  FINAL1 ;
         +Pl3:eah   FINAL1 ;

LEXICON IJ-COPULA  !!= ** __@CODE@__
 +Sg1+Use/NG:im FINAL1 ;
 +Sg2+Use/NG:ih FINAL1 ;
 +Sg3+Use/NG:ij FINAL1 ;
 +Du1+Use/NG:ij FINAL1 ;
 +Du2+Use/NG:ij FINAL1 ;
 +Du3+Use/NG:ij FINAL1 ;
 +Pl1+Use/NG:ij FINAL1 ;
 +Pl2+Use/NG:ij FINAL1 ;
 +Pl3+Use/NG:ij FINAL1 ;



