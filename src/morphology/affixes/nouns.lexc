!!€ gierehtsem:          gierehtse+N+Sg+Acc


LEXICON N_ODD_LOAN
!! * Odd-syll loanwords: lexicon @LEXNAME@
         +N+Sg+Nom:e         FINAL1    ;
   +N+Sg+Nom+PxSg1:%>amme    FINAL1    ;
   +N+Sg+Nom+PxSg2:%>adth    FINAL1    ;
         +N+Sg+Acc:e%>m      FINAL1    ;
   +N+Sg+Acc+PxSg2:%>amth    FINAL1    ;
         +N+Sg+Gen:e%>n      FINAL1    ;
   +N+Sg+Gen+PxSg1:%>anne    FINAL1    ;
   +N+Sg+Gen+PxSg2:%>anne    FINAL1    ;
   +N+Sg+Gen+PxSg3:%>anne    FINAL1    ;
         +N+Sg+Ill:%>asse    FINAL1    ; !Rule to delete Jesuse%>asse > Jesusasse
   +N+Sg+Ill+PxSg2:%>assedh  FINAL1    ;
         +N+Sg+Ine:%>isnie   FINAL1    ;
         +N+Sg+Ela:%>istie   FINAL1    ;
 +N+Sg+Ela+Err/Orth:%>istie   FINAL1    ; !Miesie Mavve
         +N+Sg+Com:%>inie    FINAL1    ;
   +N+Sg+Com+PxSg2:%>æjnedh  FINAL1    ;
         +N+Pl+Nom:%>h       FINAL1    ;
         +N+Pl+Acc:%>idie    FINAL1    ;
 +N+Pl+Acc+Err/Orth:%>idie    FINAL1    ; !Miesie Mavve
         +N+Pl+Gen:%>i       FINAL1    ;
 +N+Pl+Gen+Err/Orth:%>ij      FINAL1    ;
         +N+Pl+Ill:%>idie    FINAL1    ;
         +N+Pl+Ine:%>inie    FINAL1    ;
         +N+Pl+Ela:%>ijstie  FINAL1    ;
         +N+Pl+Com:%>igujmie FINAL1    ;
 +N+Pl+Com+Err/Orth:%>igyjmie FINAL1    ;
                +N:          N_ODD_ESS ;
       +N+Cmp/SgNom:e         R         ;
       +N+Cmp/SgGen:e%>n      R         ;
       +N+Cmp/PlGen:%>i       R         ;


LEXICON N_ODD_SG
!! ** Odd-syll sg: lexicon @LEXNAME@
!  +Nom:e         FINAL1 ; ! sjekk
         +Nom:e%^DISIMP       FINAL1 ;
   +Nom+PxSg1:%>amme          FINAL1 ;
   +Nom+PxSg2:%>adth          FINAL1 ;
         +Acc:e%>%^DISIMPm    FINAL1 ;
   +Acc+PxSg2:%>amth          FINAL1 ;
         +Gen:e%>%^DISIMPn    FINAL1 ;
   +Gen+PxSg1:%>anne          FINAL1 ;
   +Gen+PxSg2:%>anne          FINAL1 ;
   +Gen+PxSg3:%>anne          FINAL1 ;
         +Ill:%>asse          FINAL1 ; !Rule to delete Jesuse%>asse > Jesusasse
   +Ill+PxSg2:%>assedh        FINAL1 ;
         +Ine:%>isnie         FINAL1 ;
         +Ela:%>istie         FINAL1 ;
 +Ela+Err/Orth:%>%^DISIMPistie FINAL1 ; !Miesie Mavve
         +Com:%>inie          FINAL1 ;
   +Com+PxSg2:%>æjnedh        FINAL1 ;

LEXICON N_ODD_PL
!! ** Odd-syll pl: lexicon @LEXNAME@
         +Nom:%>%^DISIMPh       FINAL1 ;
         +Acc:%>idie            FINAL1 ;
 +Acc+Err/Orth:%>%^DISIMPidie    FINAL1 ; !Miesie Mavve
 +Acc+Err/Orth:%>%^DISIMPide     FINAL1 ; 
         +Gen:%>%^DISIMPi       FINAL1 ;
 +Gen+Err/Orth:%>%^DISIMPij      FINAL1 ;
         +Ill:%>idie            FINAL1 ;
 +Ill+Err/Orth:%>%^DISIMPide     FINAL1 ;       
         +Ine:%>inie            FINAL1 ;
         +Ela:%>ijstie          FINAL1 ;
        
         +Com:%>%^DISIMPigujmie FINAL1 ;
 +Com+Err/Orth:%>%^DISIMPigyjmie FINAL1 ;

LEXICON N_ODD_ESS
!! ** Odd-syll loanwords: lexicon @LEXNAME@
 +Ess:%>inie FINAL1 ;

LEXICON ÅABPETJH ! Resiproke og dim.
!! ** Default N_ODD plural lexicon: lexicon @LEXNAME@
                    +N+Pl+Nom:%>%^DISIMPh       FINAL1    ;
                    +N+Pl+Acc:%>idie            FINAL1    ;
                    +N+Pl+Gen:%>%^DISIMP        IGENSUFF  ;
                    +N+Pl+Ill:%>idie            FINAL1    ;
                    +N+Pl+Ine:%>inie            FINAL1    ;
                    +N+Pl+Ela:%>ijstie          FINAL1    ;
