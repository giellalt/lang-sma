                       +N+Ess:%>inie            FINAL1    ;
                  +N+Cmp/SgNom:e%^DISIMP         R        ;
                  +N+Cmp/PlGen:%>%^DISIMPi       R        ;
       +N+Der1+Der/Dimin+N+Pl:%»adtj            NIEJTE_PL ;
          +N+Der1+Der/Dimin+N:%»adtj            EVEN_ESS  ;
