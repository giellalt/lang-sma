!! !!Inflection for nouns ending in oe
!    ---------------------------------


!! The oe with umlaut generate the uml-ones
!! and have the non-uml ones as +Use/NG.

LEXICON N_OE_UML
 +N+Sg+Nom:oe FINAL1 ;
 +N+Sg:        N_OE_SGILL_UML ; ! Causes Umlaut
 +N+Sg+Use/NG: N_OE_SGILL_NOUML ; ! Does not cause Umlaut, do not generate
 +N:              N_OE_NO_ILL ; ! gives the other cases
         
 
LEXICON N_OE_SGILL_UML
 +Ill+Uml:e%>%^2UMLse FINAL1      ; ! Causes Umlaut
  +Ill+Uml+PxSg1:e%>%^2UMLsanne FINAL1 ;
  +Ill+Uml+PxSg2:e%>%^2UMLsadth FINAL1 ;

LEXICON N_OE_SGILL_NOUML
  +Ill:o%>se       FINAL1 ;


!! The oe without umlaut generate the non-uml-ones
!! only, naturally without +Use/NG.

LEXICON N_OE
 +N+Sg+Nom:oe FINAL1 ;
 +N:           N_OE_OBL ;
 
LEXICON N_OE_OBL 
!! Lexicon @LEXNAME@ is for the -oe nouns without umlaut
!! Illative is lifted out in order to allow for Use/NG for the
!! umlauted ones.
   +Sg: N_OE_SGILL_NOUML ;
         N_OE_NO_ILL ;

LEXICON N_OE_NO_ILL
  +Sg: N_OE_SG  ;
  +Pl: N_OE_PL  ;
       N_OE_ESS ;
  +SgNomCmp:oe    R ;
  +SgGenCmp:oe%>n R ;
  +PlGenCmp:oe%>j R ;
  +Der1+Der/Dimin+N:e%»%^2UMLtj GÅATETJE ;

