####### HFST build rules: ########
%.hfst: %.lexc
	$(AM_V_HLEXC)"$(HFST_LEXC)" \
		$(MORE_VERBOSITY) $(HFST_FORMAT) $(HFST_LEXC_FLAGS) -o $@ $^

####### Xerox build rules: #######
%.xfst: %.lexc
	$(AM_V_XFST)"$(XFST)" $(VERBOSITY) \
			-e "read lexc $^" \
			-e "save stack $@ " \
			-stop

####### Foma build rules: #######
%.foma: %.lexc
	$(AM_V_FOMA)"$(FOMA)" $(VERBOSITY) \
			-e "set lexc-align ON" \
			-e "read lexc $^" \
			-e "save stack $@ " \
			-s

