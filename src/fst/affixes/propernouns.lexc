LEXICON ERVASTI_FLAG @U.Cap.Obl@ ERVASTI ;
LEXICON ERVASTI ! Names ending in stressless vowel
    +Sg:        INAME_ODD_SG   ;
    +Sg+Nom:%>i FINAL1 ;
    +Cmp/SgNom:%>i RHyph ; ! ERVASTI-muorra, Ervasti-dállu etc. RHyph er Cmp.
    +Cmp/SgGen:%>i RHyph ;
        +Pl:          N_ODD_PL  ;
                  N_ODD_ESS ;
          +Attr:%>i FINAL1    ;

LEXICON ERVASTI_PLC-ORG_FLAG @U.Cap.Obl@ ERVASTI_PLC-ORG ;
LEXICON ERVASTI_PLC-ORG ! Names ending in stressless vowel
   +Sg: N_ODD_SG  ;
    +Cmp/SgNom:RHyph     ;
    +Cmp/SgGen:RHyph     ;
!+Pl: N_ODD_PL ;
        N_ODD_ESS ;
 +Attr: FINAL1    ;


