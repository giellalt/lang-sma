        +V+IV+Der3+Der/ahtje+V+VGen:%»ahtj%>en        FINAL1     ;
! +V+IV+Der3+Der/ahtje+V+Act:%»ahtj%>eme      FINAL1 ;
         +V+IV+Der3+Der/ahtje+V+Ger:%»ahtj%>eminie    FINAL1     ;
      +V+IV+Der3+Der/ahtje+V+PrfPrc:%»ahtj%>eme       FINAL1     ;
      +V+IV+Der3+Der/ahtje+V+ConNeg:%»ahtj%>0h        FINAL1     ;
     +V+IV+Der3+Der/ahtje+V+Sg3+Uml:%»ahtj%>%^3sUMLa  FINAL1     ;
         +V+IV+Der3+Der/ahtje+V+Sg3:%»ahtj%>i         FINAL1     ;
 +V+IV+Der3+Der/ahtje+V+Sg3+Err/Orth:%»ahtj%>%^DISIMPi FINAL1     ;

LEXICON RIHPESIDH   !!≈ * **@CODE@**,  -nidh and -sidh
                 +V+IV:               COMMON-ODD     ;
 +V+IV+Der2+Der/alla+V:%»%^adteUMLall GOEGKERDADTEDH ;

LEXICON AAJVESTIDH_TV     !!≈ * **@CODE@**, for stems ending -t-: dåajvoeht-, odd-syll with -alle- as Der2 and passive -sovvedh
                +V+TV:                DÅAJVOEHTIDH ;
 +V+TV+Der3+Der/PassL:%»ovv SOVVEDH      ;  !orrest

LEXICON DÅAJVOEHTIDH_TV  !!≈ * **@CODE@**
