LEXICON N_ODD_MEETERE
!! * Odd-syll loanwords: lexicon @LEXNAME@
+Sg:eter N_ODD_SG_Px ;
          +Sg+Nom:etere         FINAL1    ;
          +Sg+Acc:etere%>m      FINAL1    ;
          +Sg+Gen:etere%>n      FINAL1    ;
          +Sg+Ill:eter%>asse    FINAL1    ; !Rule to delete Jesuse%>asse > Jesusasse
          +Sg+Ine:eter%>isnie   FINAL1    ;
          +Sg+Ela:eter%>istie   FINAL1    ;
 +Sg+Ine+Err/Orth:eter%>esne   FINAL1    ; 
 +Sg+Ela+Err/Orth:eter%>este   FINAL1    ; 
          +Sg+Com:eter%>inie    FINAL1    ;
          +Pl+Nom:eter%>h       FINAL1    ;
          +Pl+Acc:eter%>idie    FINAL1    ;
 +Pl+Acc+Err/Orth:eter%>idie    FINAL1    ; !Miesie Mavve
          +Pl+Gen:eter%>i       FINAL1    ;
 +Pl+Gen+Err/Orth:eter%>ij      FINAL1    ;
          +Pl+Ill:eter%>idie    FINAL1    ;
          +Pl+Ine:eter%>inie    FINAL1    ;
          +Pl+Ela:eter%>ijstie  FINAL1    ;
          +Pl+Com:eter%>igujmie FINAL1    ;
 +Pl+Com+Err/Orth:eter%>igyjmie FINAL1    ;
                 :eter          N_ODD_ESS ;
       +Cmp/SgNom:ter         R         ;
       +Use/NG+Cmp/SgNom:etere         R         ;
       +Cmp/SgGen:etere%>n      R         ;
       +Cmp/PlGen:eter%>i       R         ;


LEXICON IHTELE
!! * Odd-syll loanwords on -ihtele, such as kapihtele: lexicon @LEXNAME@
         :ihtel N_ODD_LOAN ;
+Err/Orth:ittel N_ODD_LOAN ;
!+Err/Orth:ïhtel N_ODD_LOAN ;
+Err/Orth:itel  N_ODD_LOAN ;
         :ittel R          ;

LEXICON N_ODD_LOAN
!! * Odd-syll loanwords: lexicon @LEXNAME@
+Sg: N_ODD_SG_Px ;
          +Sg+Nom:e         FINAL1    ;
          +Sg+Acc:e%>m      FINAL1    ;
          +Sg+Gen:e%>n      FINAL1    ;
          +Sg+Ill:%>asse    FINAL1    ; !Rule to delete Jesuse%>asse > Jesusasse
          +Sg+Ine:%>isnie   FINAL1    ;
          +Sg+Ela:%>istie   FINAL1    ;
 +Sg+Ine+Err/Orth:%>esne   FINAL1    ; 
 +Sg+Ela+Err/Orth:%>este   FINAL1    ; 
          +Sg+Com:%>inie    FINAL1    ;
          +Pl+Nom:%>h       FINAL1    ;
          +Pl+Acc:%>idie    FINAL1    ;
 +Pl+Acc+Err/Orth:%>idie    FINAL1    ; !Miesie Mavve
          +Pl+Gen:%>i       FINAL1    ;
 +Pl+Gen+Err/Orth:%>ij      FINAL1    ;
          +Pl+Ill:%>idie    FINAL1    ;
          +Pl+Ine:%>inie    FINAL1    ;
          +Pl+Ela:%>ijstie  FINAL1    ;
          +Pl+Com:%>igujmie FINAL1    ;
 +Pl+Com+Err/Orth:%>igyjmie FINAL1    ;
                            N_ODD_ESS ;
       +Cmp/SgNom:e         R         ;
       +Cmp/SgGen:e%>n      R         ;
       +Cmp/PlGen:%>i       R         ;


LEXICON N_ODD_SG
N_ODD_SG_Px ;
!!     - Odd-syll sg: lexicon @LEXNAME@
!          +Nom:e               FINAL1 ; ! sjekk
           +Nom:e%^DISIMP       FINAL1 ;
           +Acc:e%>%^DISIMPm    FINAL1 ;
           +Gen:e%>%^DISIMPn    FINAL1 ;
           +Ill:%>asse          FINAL1 ; !Rule to delete Jesuse%>asse > Jesusasse
  +Ine+Err/Orth:%>isne         FINAL1 ;
           +Ine:%>isnie         FINAL1 ;
           +Ela:%>istie         FINAL1 ;
  +Ela+Err/Orth:%>iste         FINAL1 ;
  +Ine+Err/Orth:%>esne         FINAL1 ;
  +Ela+Err/Orth:%>este         FINAL1 ;
  +Ela+Err/Orth:%>%^DISIMPistie FINAL1 ; !Miesie Mavve
           +Com:%>inie          FINAL1 ;
  +Com+Err/Orth:%>ine          FINAL1 ;

LEXICON N_ODD_PL
!!     - Odd-syll pl: lexicon @LEXNAME@
          +Nom:%>%^DISIMPh       FINAL1 ;
          +Acc:%>idie            FINAL1 ;
 +Acc+Err/Orth:%>%^DISIMPidie    FINAL1 ; !Miesie Mavve
 +Acc+Err/Orth:%>%^DISIMPide     FINAL1 ;
          +Gen:%>%^DISIMPi       FINAL1 ;
 +Gen+Err/Orth:%>%^DISIMPij      FINAL1 ;
          +Ill:%>idie            FINAL1 ;
 +Ill+Err/Orth:%>%^DISIMPide     FINAL1 ;
          +Ine:%>inie            FINAL1 ;
          +Ela:%>ijstie          FINAL1 ;
          +Com:%>%^DISIMPigujmie FINAL1 ;
 +Com+Err/Orth:%>%^DISIMPigyjmie FINAL1 ;

LEXICON N_ODD_ESS
!!     - Odd-syll loanwords: lexicon @LEXNAME@
          +Ess:%>inie FINAL1 ;
 +Ess+Err/Orth:%>ine FINAL1 ;

LEXICON ÅABPETJH ! Resiproke og dim.
