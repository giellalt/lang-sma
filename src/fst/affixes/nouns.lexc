                     +Pl+Ela:%>ijstie          FINAL1    ;
                     +Pl+Com:%>%^DISIMPigujmie FINAL1    ;
            +Pl+Com+Err/Orth:%>%^DISIMPigyjmie FINAL1    ;
                        +Ess:%>inie            FINAL1    ;
                  +Cmp/SgNom:e%^DISIMP         R         ;
                  +Cmp/PlGen:%>%^DISIMPi       R         ;
        +Der1+Der/Dimin+N+Pl:%»adtj            NIEJTE_PL ;
